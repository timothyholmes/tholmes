{
  "name": "tholmes",
  "version": "1.0.0",
  "description": "Personal site for Tim Holmes",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "watch-sass": "nodemon -e scss -w lib/src/scss -x 'npm run build-sass'",
    "build-sass": "node-sass --output-style nested lib/src/scss/main.scss lib/src/css/main.css",
    "deploy": "npm run build-sass && npm start",
    "clean": "rm -r lib/src/css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timothyholmes/tholmes.git"
  },
  "author": "Tim Holmes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timothyholmes/tholmes/issues"
  },
  "homepage": "http://timholm.es",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-squeeze": "^4.0.0",
    "hapi": "^14.2.0",
    "inert": "^4.0.2",
    "joi": "^13.1.2"
  },
  "devDependencies": {
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0"
  }
}
